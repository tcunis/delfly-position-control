<!DOCTYPE settings SYSTEM "../settings.dtd">

<settings>
  <dl_settings>
    <dl_settings NAME="Wind-Tunnel">
      <dl_setting var="windtunnel_velocity" unit="m/s" min="0" step="0.1" max="10" module="windtunnel/windtunnel" shortname="V_w" param="WINDTUNNEL_WIND_VELOCITY" handler="setWindVelocity"/>
      <dl_setting var="windtunnel_delay" unit="s" min="0" step="1" max="5" module="windtunnel/windtunnel" shortname="T_t" param="WINDTUNNEL_DELAY_TIME"/>

      <dl_setting var="posctrl_pos_Kp" min="0" step="0.1" max="10" module="positionctrl_windtunnel" shortname="k_Vs" param="POSITIONCTRL_POSITION_PGAIN"/>
      <dl_setting var="posctrl_pos_Ki" min="0" step="0.1" max="10" module="positionctrl_windtunnel" shortname="i_Vs" param="POSITIONCTRL_POSITION_IGAIN"/>
      <dl_setting var="posctrl_pos_Kd" min="0" step="0.1" max="10" module="positionctrl_windtunnel" shortname="d_Vs" param="POSITIONCTRL_POSITION_DGAIN"/>
      <dl_setting var="posctrl_pos_Kd2" min="0" step="0.1" max="10" module="positionctrl_windtunnel" shortname="รฐ_Vs" param="POSITIONCTRL_POSITION_D2GAIN"/>
    
      <dl_setting var="velctrl_use_feedback" min="0" step="1" max="1" module="windtunnel_posctrl/windtunnel_posctrl" shortname="feedback" param="WINDTUNNEL_POSCTRL_USE_FEEDBACK"/>
      <dl_setting var="velctrl_vel_Kf" min="-5" step="0.1" max="5" module="windtunnel_posctrl/windtunnel_posctrl" shortname="f_VV" param="WINDTUNNEL_POSCTRL_VELOCITY_FGAIN"/>
    
    </dl_settings>
  </dl_settings>
</settings>
