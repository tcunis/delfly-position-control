<!DOCTYPE settings SYSTEM "../settings.dtd">

<settings>
  <dl_settings>
    <dl_settings NAME="Wind-Tunnel">
      <dl_setting var="windtunnel_velocity" unit="m/s" min="0" step="0.1" max="10" module="windtunnel/windtunnel" shortname="V_w" param="WINDTUNNEL_WIND_VELOCITY"/>
      <dl_setting var="windtunnel_delay" unit="s" min="0" step="1" max="5" module="windtunnel/windtunnel" shortname="T_t" param="WINDTUNNEL_DELAY_TIME"/>

      <dl_setting var="posctrl_vel_Kp" min="0" step="0.1" max="10" module="positionctrl_windtunnel" shortname="K_V,P" param="POSITIONCTRL_VELOCITY_PGAIN"/>
      <dl_setting var="posctrl_vel_Ki" min="0" step="0.1" max="10" module="positionctrl_windtunnel" shortname="K_V,I" param="POSITIONCTRL_VELOCITY_IGAIN"/>
    
    </dl_settings>
  </dl_settings>
</settings>
